{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "trailing-comma": [
      false
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "object-literal-sort-keys": false,
    "variable-name": [
      true,
      "ban-keywords",
      "allow-leading-underscore"
    ],
    "triple-equals": [
      true,
      "allow-undefined-check"
    ]
  },
  "rulesDirectory": []
}
